{
    "loss_fn": "dag_loss",
    "tokenizer": "helsinki_en_zh",
    "dataset": "wmt19_en_zh",
    "dataset_path": "data/wmt19_en_zh",
    "dataset_process_params": {
        "format": "torch",
        "max_en_len": 50,
        "max_zh_len": 50
    },
    "model_type": "dag",
    "use_validation": true,
    "train_dl_params": {
        "batch_size": 16,
        "coeff_fn": {
            "type": "random",
            "params": {
                "high": 4
            }
        }
    },
    "test_dl_params": {
        "batch_size": 16,
        "coeff_fn": {
            "type": "random",
            "params": {
                "high": 4
            }
        }
    },
    "model_kwargs": {
        "embed_dim": 384,
        "num_heads": 6,
        "factor": -1,
        "max_len_enc": 52,
        "max_len_dec": 208,
        "num_enc_layers": 3,
        "num_dec_layers": 3,
        "dag_heads": 6,
        "dag_phm_factor": -1,
        "dag_lm_head_factor": -1,
        "dropout": 0.12
    },
    "optimizer": "AdamW",
    "optimizer_kwargs": {
        "lr": 0.001
    },
    "summary_writer_dir": "runs/wmt19_en_zh_dag_dropout_0.12",
    "checkpoint_path": "checkpoints/wmt19_en_zh_dag_dropout_0.12/model.pt"
}